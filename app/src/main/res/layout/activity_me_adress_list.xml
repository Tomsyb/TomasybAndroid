<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tomasyb.tomasybandroid.ui.me.MeAdressListActivity">
    <android.support.v7.widget.RecyclerView
        android:id="@+id/me_adress_rv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.tomasyb.baselib.widget.sidebar.WaveSideBar
        android:id="@+id/me_adress_wsidebar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="8dp"
        android:paddingLeft="8dp"
        app:sidebar_text_color="@color/y_main_orange"/>

</RelativeLayout>
